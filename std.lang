fun and(a, b)
{
    if (a == 0)
        ret 0;
    else
    {
        if (b == 0)
            ret 0;
        ret 1;
    }
}

fun or(a, b)
{
    if (a > 0)
        ret 1;
    else
    {
        if (b > 0)
            ret 1;
        ret 0;
    }
}

fun ger(a, b)
{
    ret b < a;
}

fun geq(a, b)
{
    ret !(a < b);
}

fun leq(a, b)
{
    ret !(a > b);
}

fun neq(a, b)
{
    ret !(a == b);
}

